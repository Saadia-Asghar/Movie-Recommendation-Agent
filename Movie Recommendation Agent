    "# Import libraries\n",
    "import google.generativeai as genai\n",
    "import ipywidgets as widgets\n",
    "from IPython.display import display, clear_output\n",
    "\n",
    "# Configure API key\n",
    "genai.configure(api_key=\"AIzaSyAbUYizCqLsdrwzZLWkiWensfrCENWsMRM\")\n",
    "\n",
    "# Choose the model\n",
    "model = genai.GenerativeModel(\"models/gemini-2.5-pro\")\n",
    "\n",
    "# Define the movie taste agent\n",
    "def movie_taste_agent(movies):\n",
    "    prompt = f\"\"\"\n",
    "You are an AI agent that analyzes a person's movie taste.\n",
    "\n",
    "Input: exactly three movies.\n",
    "\n",
    "Your task:\n",
    "- Write ONE natural paragraph describing the type of movies the person likes.\n",
    "- Recommend EXACTLY three movies matching their taste.\n",
    "- Do not ask questions.\n",
    "- Do not use bullet points in the paragraph.\n",
    "- Do not repeat the input movies.\n",
    "\n",
    "Movies:\n",
    "1. {movies[0]}\n",
    "2. {movies[1]}\n",
    "3. {movies[2]}\n",
    "\n",
    "Output format:\n",
    "\n",
    "<Paragraph>\n",
    "\n",
    "Recommended Movies:\n",
    "1. Movie ‚Äì reason\n",
    "2. Movie ‚Äì reason\n",
    "3. Movie ‚Äì reason\n",
    "\"\"\"\n",
    "    response = model.generate_content(prompt)\n",
    "    return response.text\n",
    "\n",
    "# Create widgets\n",
    "movie1_widget = widgets.Text(description=\"Movie 1:\")\n",
    "movie2_widget = widgets.Text(description=\"Movie 2:\")\n",
    "movie3_widget = widgets.Text(description=\"Movie 3:\")\n",
    "button = widgets.Button(description=\"Analyze\")\n",
    "output = widgets.Output()  # This ensures output appears correctly\n",
    "\n",
    "# Button callback\n",
    "def on_button_click(b):\n",
    "    with output:\n",
    "        clear_output()  # Clears previous output\n",
    "        movies = [movie1_widget.value, movie2_widget.value, movie3_widget.value]\n",
    "        if all(movies):\n",
    "            print(\"üé¨ Analyzing your movie taste...\\n\")\n",
    "            print(movie_taste_agent(movies))\n",
    "        else:\n",
    "            print(\"‚ö†Ô∏è Please enter all three movies!\")\n",
    "\n",
    "button.on_click(on_button_click)\n",
    "\n",
    "# Display everything\n",
    "display(movie1_widget, movie2_widget, movie3_widget, button, output)\n"
   ]
  }
